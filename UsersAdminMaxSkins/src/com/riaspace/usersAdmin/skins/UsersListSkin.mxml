<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:fc="http://ns.adobe.com/flashcatalyst/2009"
		xmlns:graphics="assets.graphics.*"
		width="304" height="425" fc:resizable="true">
	<fx:Metadata>[HostComponent("spark.components.List")]</fx:Metadata>
	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>
	<s:DataGroup id="dataGroup" x="0" y="0" width="284" height="425" clipAndEnableScrolling="true"
				 itemRenderer="com.riaspace.usersAdmin.skins.UsersListItemRenderer">
		<s:layout>
			<s:VerticalLayout/>
		</s:layout>
	</s:DataGroup>
	<s:VScrollBar includeIn="normal" x="289" y="0" fixedThumbSize="true"
				  skinClass="com.riaspace.usersAdmin.skins.UsersListScrollbarSkin" viewport="{dataGroup}"/>
</s:Skin>
