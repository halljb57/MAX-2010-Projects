<?xml version="1.0" encoding="utf-8"?>
<s:WindowedApplication xmlns:fx="http://ns.adobe.com/mxml/2009" 
					   xmlns:s="library://ns.adobe.com/flex/spark" 
					   xmlns:mx="library://ns.adobe.com/flex/mx" 
					   xmlns:views="com.riaspace.usersAdmin.views.*"
					   xmlns:swiz="http://swiz.swizframework.org" 
					   xmlns:configs="com.riaspace.usersAdmin.configs.*"
					   width="330" height="520"
					   title="Users Admin (Powered by Swiz)">
	
	<fx:Declarations>
		<swiz:Swiz>
			<swiz:beanProviders>
				<configs:Config />
			</swiz:beanProviders>
			<swiz:config>
				<swiz:SwizConfig
					eventPackages="com.riaspace.usersAdmin.events"
					viewPackages="com.riaspace.usersAdmin.views" />
			</swiz:config>
		</swiz:Swiz>
	</fx:Declarations>
	
	<views:MainView id="mainView" left="0" top="0" bottom="0" right="0" />
	
</s:WindowedApplication>
